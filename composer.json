{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "althinect/filament-spatie-roles-permissions": "3.x-dev",
        "awcodes/filament-quick-create": "^4.0",
        "awcodes/shout": "^3.0",
        "aws/aws-sdk-php": "^3.320",
        "bezhansalleh/filament-language-switch": "^4.0",
        "bezhansalleh/filament-panel-switch": "^2.0",
        "calebporzio/sushi": "^2.5",
        "charrafimed/global-search-modal": "^4.0",
        "doctrine/dbal": "^3.8",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "filament/spatie-laravel-settings-plugin": "^4.0",
        "guava/filament-icon-picker": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^2.0",
        "lara-zeus/spatie-translatable": "^1.0",
        "laravel-notification-channels/webhook": "^2.7",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.29",
        "laravel/octane": "^2.3",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.12",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.29",
        "league/flysystem-sftp-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "martinpetricko/filament-sentry-feedback": "^2.0",
        "mpdf/mpdf": "^8.2",
        "mvenghaus/filament-plugin-schedule-monitor": "^3.0",
        "njxqlus/filament-lightbox": "^3.0",
        "njxqlus/filament-progressbar": "^2.0",
        "parfaitementweb/filament-country-field": "^2.0",
        "predis/predis": "^2.2",
        "pxlrbt/filament-environment-indicator": "^3.0",
        "pxlrbt/filament-spotlight": "^2.0",
        "sentry/sentry-laravel": "^4.15",
        "shuvroroy/filament-spatie-laravel-health": "^3.0",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/laravel-backup": "^9.2",
        "spatie/laravel-enum": "*",
        "spatie/laravel-permission": "^6.4",
        "spatie/laravel-schedule-monitor": "^3.6",
        "spatie/laravel-settings": "^3.4",
        "spatie/security-advisories-health-check": "^1.1",
        "symfony/browser-kit": "^7.0",
        "symfony/http-client": "^7.0",
        "tightenco/ziggy": "^2.0",
        "tomatophp/filament-developer-gate": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.46",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {

    }
}
